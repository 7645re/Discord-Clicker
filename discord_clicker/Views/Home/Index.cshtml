<div class="wrapper">
    <main class="main">
        <h1 class="clicker_counter">0</h1>
        <h1 class="cps_counter">0</h1>
        <img ondragstart="return false;" class="clicker_button" src="images/env/wampus.png">
    </main>
    <aside>
        <div class="arrows">
            <button class="btn btn-primary game-button btn-builds shadow-none" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasBottomBuilds" aria-controls="offcanvasBottom">
                <img ondragstart="return false;" class="arrow build-arrow" src="images/env/build-arrow.png">
            </button>
            <button class="btn btn-primary game-button btn-upgrades shadow-none" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasBottomUpgrades" aria-controls="offcanvasBottom">
                <img ondragstart="return false;" class="arrow upgrade-arrow" src="images/env/upgrade-arrow.png">
            </button>
            <button class="btn btn-primary game-button btn-profile shadow-none" data-bs-toggle="modal" data-bs-target="#exampleModal" type="button">
                <img ondragstart="return false;" class="arrow profile-arrow" src="images/env/profile-arrow.png">
            </button>
            <button class="btn btn-primary game-button btn-leaderboard shadow-none" type="button">
                <img ondragstart="return false;" class="arrow leader-arrow" src="images/env/leader-arrow.png">
            </button>
        </div>
        <img ondragstart="return false;" class="pillar" src="images/env/столб.png">
    </aside>
</div>
<div class="offcanvas offcanvas-bottom builds" tabindex="-1" id="offcanvasBottomBuilds" aria-labelledby="offcanvasBottomLabel">
